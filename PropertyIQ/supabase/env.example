# Supabase Environment Variables for PropertyIQ
# Copy this file to .env and fill in your actual values

# Get these from your Supabase project dashboard
# https://supabase.com/dashboard/project/YOUR_PROJECT_ID/settings/api

# Supabase Project URL
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co

# Supabase Anonymous/Public Key (safe for browser)
SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (server-side only, keep secret!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database connection string (for migrations and direct access)
DATABASE_URL=postgresql://postgres:your-password@db.YOUR_PROJECT_REF.supabase.co:5432/postgres

# Optional: Custom domain if you have one
# SUPABASE_CUSTOM_DOMAIN=https://api.yourdomain.com

# Edge Functions specific environment variables
# These will be available in your Edge Functions

# Storage bucket names
STORAGE_UPLOADS_BUCKET=uploads
STORAGE_REPORTS_BUCKET=reports

# Rate limiting settings
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000

# Cache settings
CACHE_TTL_SECONDS=3600
MAX_CACHE_SIZE=10000

# Processing limits
MAX_BATCH_SIZE=100
MAX_CONCURRENT_PROCESSING=10
MAX_GEOIDS_PER_REPORT=100

# Report generation settings
DEFAULT_REPORT_FORMAT=json
ENABLE_PDF_GENERATION=true
ENABLE_EXCEL_GENERATION=true

# Error reporting (optional)
# SENTRY_DSN=your-sentry-dsn-here

# External API keys (if needed)
# GOOGLE_MAPS_API_KEY=your-google-maps-key
# MAPBOX_ACCESS_TOKEN=your-mapbox-token

# Feature flags
ENABLE_CACHING=true
ENABLE_RATE_LIMITING=true
ENABLE_DETAILED_LOGGING=false

# Development settings (remove in production)
DEBUG_MODE=false
ALLOW_CORS_ALL_ORIGINS=true
